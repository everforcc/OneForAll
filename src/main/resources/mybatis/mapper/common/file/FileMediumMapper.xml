<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cc.dawn.common.file.dao.FileMediumMapper">

    <insert id="insert">
        INSERT INTO `cc_file_medium` (
            `uuid`,
            `uname`,
            `filemediumenum`,
            `path`,
            `effect`,
            `status`
        )
        VALUES
            (
                #{uuid},
                #{uname},
                #{fileMediumEnum},
                #{path},
                #{effect},
                #{status}
            );
    </insert>

</mapper>