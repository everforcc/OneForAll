<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cc.dawn.common.file.dao.FileObjMapper">

    <insert id="insert">
        INSERT INTO `cc_file_obj` (
            `uuid`,
            `uname`,
            `rname`,
            `contentType`,
            `size`,
            `effect`,
            `status`
        )
        VALUES
            (
                #{uuid},
                #{uname},
                #{rname},
                #{contentType},
                #{size},
                #{effect},
                #{status}
            );
    </insert>

</mapper>