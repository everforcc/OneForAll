<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cc.dawn.common.dao.WebSiteTagDtoMapper">

    <!-- 列表查询 -->
    <select id="selectAll" resultType="cn.cc.dawn.common.dto.WebSiteTagDto">
        select * from cc_websitetag;
    </select>

    <insert id="insert">
        INSERT INTO `oneforall`.`cc_websitetag` (
            `webrootid`,
            `weburl`,
            `webname`,
            `webdataid`
        )
        VALUES
            (
                #{webrootid},
                #{weburl},
                #{webname},
                #{webdataid}
            );
    </insert>

</mapper>